{
  "editor.quickSuggestions": false,
  "files.exclude": {
    "**/.git": true,
    ".sass-cache": true,
    "_sass/.sass-cache": true,
    ".pygments-cache": true,
    "_site": true,
    ".bundle": true,
    "_gh-pages": true
  },
  "prettier.printWidth": 116,
  "[markdown]": {
    "editor.formatOnSave": true,
    "editor.wordWrap": "on",
    "editor.quickSuggestions": false
  },
  "spellchecker.language": "en_US",
  
  "spellchecker.documentTypes": ["markdown", "latex", "plaintext"],
  "spellchecker.ignoreRegExp": [
    "/\\(.*\\.(jpg|jpeg|png|md|gif|JPG|JPEG|PNG|MD|GIF)\\)/g",
    "/((http|https|ftp|git)\\S*)/g"
  ],
  "spellchecker.ignoreFileExtensions": [],
  "spellchecker.checkInterval": 5000,
